{
  "name": "mui-phone-textfield",
  "version": "2.1.0",
  "description": "A phone number input for Material-UI.",
  "main": "dist/cjs/bundle.js",
  "module": "dist/esm/bundle.js",
  "types": "dist/esm/bundle.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf examples/public dist",
    "start": "npm run clean && concurrently -k npm:start:*",
    "start:dist": "rollup -cw --environment NODE_ENV:development",
    "start:examples": "webpack serve --env NODE_ENV=development -c examples/webpack.config.js",
    "dev": "npm run clean && npm run dev:dist && npm run dev:examples",
    "dev:dist": "rollup -c --environment NODE_ENV:development",
    "dev:examples": "webpack --env NODE_ENV=development -c examples/webpack.config.js",
    "build": "npm run clean && npm run build:dist && npm run build:examples",
    "build:dist": "rollup -c --environment NODE_ENV:production",
    "build:examples": "webpack --env NODE_ENV=production -c examples/webpack.config.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soufyakoub/mui-phone-textfield.git"
  },
  "keywords": [
    "phone",
    "input",
    "material-ui",
    "react",
    "material design",
    "react-component"
  ],
  "author": {
    "name": "Soufiane Yakoubi",
    "email": "splentercell.1997@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soufyakoub/mui-phone-textfield/issues"
  },
  "homepage": "https://github.com/soufyakoub/mui-phone-textfield#readme",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@material-ui/core": "^4.11.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/react-window": "^1.8.2",
    "@wessberg/rollup-plugin-ts": "^1.3.8",
    "babel-loader": "^8.2.2",
    "cldr-localenames-full": "^38.0.0",
    "concurrently": "^5.3.0",
    "cssnano": "^4.1.10",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.6",
    "postcss": "^8.2.0",
    "postcss-assets": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.34.2",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^17.3.0",
    "typescript": "^4.1.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "peerDependencies": {
    "@material-ui/core": ">= 4.x.x",
    "react": ">= 17.x.x"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "libphonenumber-js": "^1.9.6",
    "prop-types": "^15.7.2",
    "react-window": "^1.8.6",
    "tslib": "^2.0.3"
  }
}
